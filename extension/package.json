{
	"name": "coditime",
	"displayName": "CodiTime",
	"description": "CodiTime tracks your time spent in different projects and files in Visual Studio Code.",
	"author": "Linus Romland (linusromland)",
	"publisher": "linusromland",
	"homepage": "https://github.com/CodiTime/CodiTime-vscode#readme",
	"license": "MIT",
	"version": "0.0.3",
	"pricing": "Free",
	"icon": "images/icon.png",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodiTime/CodiTime-vscode.git"
	},
	"bugs": {
		"url": "https://github.com/CodiTime/CodiTime-vscode/issues"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"category": "CodiTime",
				"command": "coditime.apiKey",
				"title": "API Key"
			},
			{
				"category": "CodiTime",
				"command": "coditime.apiUrl",
				"title": "API Url"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"lint:fix": "eslint src --ext ts --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check ."
	},
	"dependencies": {
		"axios": "^1.2.2",
		"prettier": "^2.8.1"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/node": "16.x",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"glob": "^8.0.3",
		"typescript": "^4.9.3"
	}
}
